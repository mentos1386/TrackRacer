{
  "name": "fri-rg",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --inline --progress --watch --open",
    "server:prod": "cross-env NODE_ENV=production webpack-dev-server --port 3000 --host 0.0.0.0 --hot --inline --progress --profile --watch --open --content-base dist/",
    "build": "npm run build:dev",
    "build:dev": "webpack --progress --profile --color --display-error-details --display-cached",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --profile --color --display-error-details --display-cached --bail",
    "clean": "npm cache clear && rimraf -- dist"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/lodash": "^4.14.70",
    "@types/node": "^8.0.51",
    "awesome-typescript-loader": "^3.2.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.0",
    "eslint": "^4.2.0",
    "expose-loader": "^0.7.3",
    "file-loader": "^1.1.5",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "rimraf": "^2.6.1",
    "shader-loader": "^1.3.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-node": "^3.3.0",
    "ts-shader-loader": "^1.0.4",
    "tslint": "^5.5.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "2.5.1",
    "@types/cannon": "^0.1.2",
    "@types/gl-matrix": "^2.4.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1"
  },
  "dependencies": {
    "cannon": "^0.6.2",
    "gl-matrix": "^2.4.0",
    "lodash": "^4.17.4",
    "webgl-obj-loader": "^1.0.0",
    "webpack-obj-loader": "^1.0.4"
  }
